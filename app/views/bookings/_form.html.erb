        <%= simple_form_for [@rental, @booking] do |f| %>
          <%= f.label :start_date, "Start Date" %>
          <%= f.date_field :start_date, class: 'form-control datepicker', id: 'booking_start_date' %>
          <%= f.label :end_date, "End Date" %>
          <%= f.date_field :end_date, class: 'form-control datepicker', id: 'booking_end_date' %>
          <%= f.button :submit, 'Book Now', class: 'btn btn-primary my-3 w-100' %>
        <% end %>
